 {
	"manifest_version": 2,
	"name": "Christmas Countdown",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["jquery-3.2.1.min.js", "content.js","popup.js"]
		}
	],
	"browser_action": {
		"default_icon": "christmastree.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"]
	}
	
}